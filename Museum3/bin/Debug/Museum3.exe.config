<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<!-- Интервал на главном слайдере в секундах-->
		<add key="MainSliderInterval" value="5"/>
		<!-- Время бездействия для главного слайдера в минутах-->
		<add key="MainSliderHold" value="1"/>
		<!-- Пароль закрытия приложения-->
		<add key="Password" value="123456"/>
		<!-- Интервал на слайдере просмоторщика в секундах-->
		<add key="GallerySliderInterval" value="5"/>
		<!-- Размер шрифта описания просмотрщика в пт-->
		<add key="GalleryDescFontSize" value="16"/>
		<!-- Размер шрифта описания на главном экране в пт-->
		<add key="MainDescFontSize" value="12"/>
		<!-- Размер шрифта названия категории на главном экране в пт-->
		<add key="MainTitleFontSize" value="16"/>
	</appSettings>
</configuration>